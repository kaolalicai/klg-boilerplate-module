{
  "name": "{{name}}",
  "version": "1.0.2",
  "description": "{{description}}",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/tsc -d -p ./",
    "test": "NODE_ENV=test mocha -r ts-node/register -r source-map-support/register test/**/*.test.ts",
    "cov": "NODE_ENV=test nyc npm test",
    "ci": "npm run lint && npm run cov"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage",
      "dist",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git.oschina.net/kalengo/{{name}}"
  },
  "keywords": [
    "log",
    "klg"
  ],
  "author": "{{author}}",
  "license": "ISC",
  "dependencies": {
    "tracer": "^0.8.12"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/should": "^13.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.2",
    "nyc": "^11.4.1",
    "should": "^13.2.1",
    "source-map-support": "^0.5.3",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  }
}